{
  "version": "0.19.8",
  "license": "Apache-2.0",
  "main": "node_modules/expo/AppEntry.js",
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "eject": "expo eject",
    "lint": "eslint **/*.ts **/*.tsx",
    "start-env": "cp .$ENV.env .env && expo start -c",
    "start-consumer": "FLAVOR=consumer npm run start-env",
    "start-consumer-dev": "ENV=dev npm run start-consumer",
    "start-consumer-local": "ENV=dev FIREBASE_EMULATOR=true npm run start-consumer",
    "start-consumer-staging": "ENV=staging npm run start-consumer",
    "start-courier": "FLAVOR=courier npm run start-env",
    "start-courier-dev": "ENV=dev npm run start-courier",
    "start-courier-local": "ENV=dev FIREBASE_EMULATOR=true npm run start-courier",
    "start-courier-staging": "ENV=staging npm run start-courier",
    "inc-version": "node ./version-inc",
    "prepare-deploy": "git checkout $ENV && git pull && cp .$ENV.env .env",
    "deploy": "expo publish --release-channel $RELEASE_CHANNEL",
    "deploy-courier:dev": "ENV=dev FLAVOR=courier npm run prepare-deploy && npm version ${VERSION:-patch} && FLAVOR=courier RELEASE_CHANNEL=dev npm run deploy",
    "deploy-courier:staging": "ENV=staging FLAVOR=courier npm run prepare-deploy && FLAVOR=courier RELEASE_CHANNEL=staging npm run deploy",
    "deploy-courier:live": "ENV=live FLAVOR=courier npm run prepare-deploy && FLAVOR=courier RELEASE_CHANNEL=live-v0 npm run deploy",
    "deploy-consumer:dev": "ENV=dev FLAVOR=consumer npm run prepare-deploy && npm version ${VERSION:-patch} && FLAVOR=consumer RELEASE_CHANNEL=dev npm run deploy",
    "deploy-consumer:staging": "ENV=staging FLAVOR=consumer npm run prepare-deploy && FLAVOR=consumer RELEASE_CHANNEL=staging npm run deploy",
    "deploy-consumer:live": "ENV=live FLAVOR=consumer npm run prepare-deploy && FLAVOR=consumer RELEASE_CHANNEL=live-v0 npm run deploy",
    "build-courier:live": "FLAVOR=courier expo build:android -t app-bundle --release-channel live-v0",
    "build-consumer:live": "FLAVOR=consumer expo build:android -t app-bundle --release-channel live-v0",
    "clear-cache": "watchman watch-del-all && rm -rf node_modules/ && rm -f package-lock.json && npm install --legacy-peer-deps",
    "version": "node ./version-inc && git add version.json",
    "postversion": "git push && git push --tags && git fetch"
  },
  "dependencies": {
    "@appjusto/types": "file:../types",
    "@expo/react-native-action-sheet": "^3.8.0",
    "@expo/vector-icons": "^12.0.0",
    "@fnando/cnpj": "^1.0.2",
    "@fnando/cpf": "^1.0.2",
    "@mapbox/polyline": "^1.1.1",
    "@react-native-community/async-storage": "~1.12.0",
    "@react-native-community/masked-view": "0.1.10",
    "@react-native-community/slider": "3.0.3",
    "@react-native-community/viewpager": "4.2.0",
    "@react-navigation/bottom-tabs": "^5.7.1",
    "@react-navigation/native": "^5.6.1",
    "@react-navigation/stack": "^5.6.2",
    "@types/algoliasearch": "^3.34.11",
    "@types/googlemaps": "^3.43.2",
    "@types/lodash": "^4.14.158",
    "algoliasearch": "^4.8.3",
    "axios": "^0.21.1",
    "dayjs": "^1.8.36",
    "dotenv": "^8.2.0",
    "expo": "^40.0.0",
    "expo-analytics-segment": "~9.1.0",
    "expo-app-loading": "^1.0.1",
    "expo-asset": "~8.2.1",
    "expo-camera": "~9.1.0",
    "expo-constants": "~9.3.3",
    "expo-device": "~2.4.0",
    "expo-font": "~8.4.0",
    "expo-image-manipulator": "~8.4.0",
    "expo-image-picker": "~9.2.0",
    "expo-linking": "~2.0.1",
    "expo-localization": "~9.1.0",
    "expo-location": "~10.0.0",
    "expo-media-library": "~10.0.0",
    "expo-notifications": "~0.8.2",
    "expo-permissions": "~10.0.0",
    "expo-splash-screen": "~0.8.1",
    "expo-task-manager": "~8.6.0",
    "firebase": "7.9.0",
    "geofirestore": "^4.2.1",
    "geokit": "^1.1.0",
    "i18n-js": "^3.7.1",
    "lodash": "^4.17.19",
    "marked": "^2.0.1",
    "nanoid": "^3.1.10",
    "normalizr": "^3.6.0",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "react-native": "https://github.com/expo/react-native/archive/sdk-40.0.1.tar.gz",
    "react-native-gesture-handler": "^1.8.0",
    "react-native-keyboard-aware-scroll-view": "^0.9.3",
    "react-native-maps": "0.27.1",
    "react-native-safe-area-context": "3.1.9",
    "react-native-screens": "~2.15.2",
    "react-native-svg": "12.1.0",
    "react-native-web": "~0.13.12",
    "react-native-webview": "11.0.0",
    "react-query": "3.5.11",
    "react-redux": "^7.2.0",
    "redux": "^4.0.5",
    "redux-thunk": "^2.3.0",
    "reselect": "^4.0.0",
    "sentry-expo": "^3.0.4"
  },
  "devDependencies": {
    "@babel/core": "~7.9.0",
    "@types/i18n-js": "^3.0.3",
    "@types/mapbox__polyline": "^1.0.0",
    "@types/marked": "^2.0.1",
    "@types/react": "~16.9.35",
    "@types/react-native": "~0.63.2",
    "@types/react-redux": "^7.1.9",
    "@typescript-eslint/eslint-plugin": "^3.7.0",
    "@typescript-eslint/parser": "^3.7.0",
    "babel-preset-expo": "8.3.0",
    "eslint": "^7.5.0",
    "eslint-config-universe": "^4.0.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "prettier": "^2.0.5",
    "typescript": "~4.0.0"
  },
  "private": true
}
